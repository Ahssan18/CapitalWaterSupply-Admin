<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mazy.capitalwatersupplyadmin">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mazy.capitalwatersupplyadmin.activities.ForgotPasswordActivity"></activity>
        <activity android:name="com.mazy.capitalwatersupplyadmin.activities.AddressesActivity"></activity>
        <activity android:name="com.mazy.capitalwatersupplyadmin.activities.TankersActivity" />
        <activity android:name="com.mazy.capitalwatersupplyadmin.activities.ManageTankersActivity" />
        <activity android:name="com.mazy.capitalwatersupplyadmin.activities.LoginActivity" />
        <activity
            android:name="com.mazy.capitalwatersupplyadmin.activities.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mazy.capitalwatersupplyadmin.activities.MainActivity" />
    </application>

</manifest>